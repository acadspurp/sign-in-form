<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.regform" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Connect</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />

    <platform name="android">
        <icon density="mdpi" src="res/android/mipmap-mdpi/connect.png" />
        <icon density="hdpi" src="res/android/mipmap-hdpi/connect.png" />
        <icon density="xhdpi" src="res/android/mipmap-xhdpi/connect.png" />
        <icon density="xxhdpi" src="res/android/mipmap-xxhdpi/connect.png" />
        <icon density="xxxhdpi" src="res/android/mipmap-xxxhdpi/connect.png" />
    </platform>
</widget>
